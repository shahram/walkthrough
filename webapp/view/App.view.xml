    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true">
    <!-- <Text text="Hello Mars!"/> -->
    <Shell>
    <App class="myAppDemoWT">
        <pages>
            <Page title="{i18n>homePageTitle}">
                <headerContent>
                    <Button
                        icon="sap-icon://hello-world"
                        press=".onOpenDialog"/>
                </headerContent>
                <content>
                   <mvc:XMLView 
